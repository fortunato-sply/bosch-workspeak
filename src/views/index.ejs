<%- include("parts/header.ejs") %>
  <link rel="stylesheet" href="css/postModal.css">
  <title>WorkSpeak | HomePage</title>
  </head>

  <body>
    <header>
      <div class="header-box">
        <a href="home">
          <img src="img/bosch.png" alt="BOSCH">
        </a>
        <form action="logout" method="get" class="signout">
          <button type="submit" class="signout-btn"><i class="ph-sign-out"></i></button>
        </form>
      </div>
    </header>
    <main id="main">
      <section class="profile-container">
        <div class="background" style="background: url(data:image/jpeg;base64,); background-repeat: no-repeat; background-size: cover;
        background-clip: border-box;
        background-position: center;">
          <form action="/userProfile/<%= getUser.IDUser %>" method="post">
            <button type="submit" class="img-btn">
              <img src="<% if(getUser.Picture === null){ %>
              <%= " img/user.png" %>
              <% } else{ %>
                <%= "img/" +getUser.Picture %>
                  <% } %>" alt="">
            </button>
          </form>
        </div>
        <div class="role">
          <strong>
            <%= getUser.Name %>
          </strong>
          <p>
            <% if(getUser.Role !=null){ %>
              <%= getUser.Role %>
                <% } %>
          </p>
        </div>
        <div class="afterline" style="width: 10%; margin-top: 10px; height: 1px;"></div>
        <form class="sala col" action="/edit" method="get">
          <button type="submit" class="edit-btn ph-pencil-simple">Edit Profile</button>
        </form>
      </section>
      <section class="feed-container">
        <% for(let i=0;i<posts.length;i++) { %>
          <% if(posts[i].General==true || posts[i]['Sector.IDSector']==getUser.IDSector) { %>
            <div class="post">
              <div class="user-info">
                <div class="user-box">
                  <form class="user" action="/userProfile/<%= posts[i]['User.IDUser'] %>" method="post">
                    <button type="submit" class="user-btn">
                      <img src=<% if(posts[i]['User.Picture']===null){ %>
                        <%= "img/user.png" %>
                      <% } else{ %>
                        <%= "img/" + posts[i]['User.Picture'] %>
                      <% } %> alt="">
                      <div class="name">
                        <strong>
                          <%= posts[i]['User.Name'] %>
                        </strong>
                        <p>
                          <%= posts[i]['User.Role'] %> @
                          <%= posts[i]['Sector.Name'] %>
                        </p>
                      </div>
                    </button>
                  </form>
                </div>
                <div class="timer">
                  <h3>
                    <%= posts[i].updatedAt.toLocaleString().substr(0, 10) %>
                  </h3>
                  <% if(getUser.IDUser==posts[i]['User.IDUser']) { %>
                    <div class="options">
                      <i class="ph-dots-three-vertical dots"></i>
                      <div class="options-modal">
                        <form action="/editPost/<%=posts[i].IDPost%>" method="post" class="editform">
                          <button type="submit" class="editform-btn"><i class="ph-pencil-simple"></i> Edit</button>
                        </form>
                        <form action="/deletePost/<%=posts[i].IDPost%>" method="post" class="editform del">
                          <button type="submit" class="editform-btn del"><i class="ph-trash"></i> Delete</button>
                        </form>
                      </div>
                    </div>
                  <% } %>
                </div>
              </div>
              <div class="content">
                <%= posts[i].Content %>
              </div>
              <span class="afterline"></span>
              <div class="comment-box">
                <span>Leave your feedback</span>
                <form action="addComment" method="post">
                  <input type="text" name="postID" value="<%= posts[i].IDPost %>" style="display: none;" readonly>
                  <div class="content-box" style="height: 130px;">
                    <textarea name="content" maxlength="300" id="textComment" cols="30" rows="10"
                      placeholder="Write a comment..." required></textarea>
                    <span id="countComment">0/300</span>
                  </div>
                  <button type="submit" class="submit-btn">Publish</button>
                </form>
              </div>
              <div class="comments">
                <%var show=false; for(let x=0;x<comments.length; x++){ %>
                  <%if(comments[x].IDPost==posts[i].IDPost) { %>
                    <% if(!show) { %>
                      <span class="afterline cc"></span>
                      <p>Comments</p>
                      <% show=true; %>
                    <% } %>
                    <div class="comment">
                            <div class="top">
                        <form class="info-user" action="/userProfile/<%= posts[i]['User.IDUser'] %>" method="post">
                          <button type="submit" class="user-btn">
                            <img src=<% if(comments[x]['User.Picture']===null){ %>
                              <%= "img/user.png" %>
                            <% } else{ %>
                              <%= "img/" + comments[x]['User.Picture'] %>
                            <% } %> alt="">
                            <div class="name">
                              <strong style="font-size: 1rem;">
                                <%= comments[x]['User.Name'] %>
                              </strong>
                              <p>
                                <%= comments[x]['User.Role'] %> @ <%= comments[x]['Sector.Name'] %>
                              </p>
                            </div>
                          </button>
                        </form>
                              <form action="delcomment">
                                <button type="submit" style="border: 0; background-color: transparent;">
                                  <i class="ph-trash delbtn"></i>
                                </button>
                              </form>
                            </div>
                      <div class="comment-content">
                        <%= comments[x].Content %>
                      </div>
                    </div>
                  <% } %>
                <% } %>
              </div>
            </div>
          <% } %>
        <% } %>
      </section>
    </main>
    <button type="submit" class="new-post-btn" onclick="openPostModal()"><i class="ph-plus"></i></button>
    <div class="post-modal hidden" id="postModal">
      <h1>New post</h1>
      <form action="/newPost" method="post" class="post-form">
        <div class="content-box">
          <textarea name="content" maxlength="500" id="textContent" cols="30" rows="10" placeholder="Type here..."
            class="txt-area" required></textarea>
          <p id="countContent">0/500</p>
        </div>
        <div class="radio">
          <div>
            <input type="radio" name="sector" id="sector" value="sectorId" checked>
            <label for="sector">
              <div class="radio-btn"></div>
              <span>Sector</span>
            </label>
          </div>
          <div>
            <input type="radio" name="sector" id="general" value="general">
            <label for="general">
              <div class="radio-btn"></div>
              <span>General</span>
            </label>
          </div>
        </div>
        <button type="submit" class="submit-btn" style="width: 40%; margin-top: 20px;">Publish</button>
      </form>
    </div>
    <script src="js/postModal.js"></script>
    <script src="js/textSize.js"></script>
    <%- include("parts/footer.ejs") %>